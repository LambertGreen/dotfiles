{
    "redhat.telemetry.enabled": true,
    "vim.easymotion": true,
    "vim.useSystemClipboard": true,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<space>"
            ],
            "commands": [
                "vspacecode.space"
            ]
        },
        {
            "before": [
                ","
            ],
            "commands": [
                "vspacecode.space",
                {
                    "command": "whichkey.triggerKey",
                    "args": "m"
                }
            ]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<space>"
            ],
            "commands": [
                "vspacecode.space"
            ]
        },
        {
            "before": [
                ","
            ],
            "commands": [
                "vspacecode.space",
                {
                    "command": "whichkey.triggerKey",
                    "args": "m"
                }
            ]
        }
    ],
    "window.zoomLevel": 2,
    "codeGenie.advanced.currentVersion": "6.1.0",
    "codeGenie.advanced.chat.enabled": true,
    "codeGenie.settings.enabled": true,
    "git.openRepositoryInParentFolders": "always",
    "editor.fontFamily": "'Aporetic Sans Mono', 'Symbols Nerd Font Mono'"
}