# -*- gdb-script -*-

set startup-with-shell off
set print pretty on
set print object on
set print static-members on
set print vtbl on
set print demangle on
set pagination off
set auto-load local-gdbinit on
set debug-file-directory /usr/lib/debug
set substitute-path external /home/tsi/lgreen/dev/work/blitz/repositories

add-auto-load-safe-path /home/tsi/lgreen/dev/my/dotfiles/gdb/dot-gdbinit
add-auto-load-safe-path /usr/lib64/libthread_db-1.0.so

python
import sys, os.path
sys.path.insert(0, os.path.expanduser('~/.gdb'))
import TableauPrinters
TableauPrinters.register_printers()
end
